{
  "name": "web",
  "license": "MIT",
  "type": "module",
  "scripts": {
    ".env": "source scripts/dotenv.sh &&",
    "dev": "pnpm .env vinxi dev",
    "prod": "pnpm .env pnpm build && pnpm serve",
    "build": "pnpm .env vinxi build",
    "start": "pnpm .env vinxi start",
    "serve": "cd .output && npx http-server public -p 3000",
    "cap": "pnpm .env ./ts-run node_modules/@capacitor/cli/bin/capacitor",
    "android": "pnpm android:build && pnpm android:start",
    "android:build": "pnpm .env scripts/android/android-build.sh && pnpm cap sync && scripts/android/android-zip.sh",
    "android:start": "pnpm .env ./scripts/android/android-run.sh",
    "android:log": "adb logcat -s 'AndroidRuntime:E' -s '${ANDROID_APPID}:I' -s 'Capacitor:I' -s 'Capacitor/Console:I'",
    "e2e": "pnpm exec playwright test",
    "e2e:chrome": "pnpm .env pnpm exec playwright test --project=chromium",
    "e2e:android": "pnpm .env pnpm exec playwright test --project=webview"
  },
